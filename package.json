{
  "name": "@knovator/homepage-builder-node",
  "version": "0.1.0",
  "license": "MIT",
  "author": "chavda-bhavik (knovator)",
  "main": "dist/index.js",
  "module": "dist/homepage-builder-node.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": true
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/homepage-builder-node.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/homepage-builder-node.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "dts-cli": "^1.5.2",
    "husky": "^8.0.1",
    "size-limit": "^8.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-list-endpoints-descriptor": "^1.0.13",
    "joi": "^17.6.0",
    "mongoose": "^6.5.1",
    "mongoose-paginate-v2": "^1.7.0"
  }
}
